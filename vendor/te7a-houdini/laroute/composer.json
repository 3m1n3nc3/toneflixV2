{
    "name": "te7a-houdini/laroute",
    "description": "Access Laravels URL/Route helper functions, from JavaScript.",
    "keywords"  : ["laravel", "routes", "routing", "javascript"],
    "authors": [
        {
            "name": "Ahmed Abd El Ftah",
            "email": "ahmedabdelftah95165@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Aaron Lord",
            "email": "hello@aaronlord.is"
        }
    ],
    "license" : "MIT",
    "require": {
        "php" : ">=7.1.3",
        "illuminate/support" : "~5.8.0|~6.0|~7.0|~8.0",
        "illuminate/routing": "~5.8.0|~6.0|~7.0|~8.0",
        "illuminate/console": "~5.8.0|~6.0|~7.0|~8.0",
        "illuminate/config": "~5.8.0|~6.0|~7.0|~8.0",
        "illuminate/filesystem": "~5.8.0|~6.0|~7.0|~8.0"
    },
    "require-dev" : {
        "orchestra/testbench": "^3.5|~4.0|~5.0",
        "mockery/mockery": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Te7aHoudini\\Laroute\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Te7aHoudini\\Laroute\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Te7aHoudini\\Laroute\\LarouteServiceProvider"
            ]
        }
    }
}
